[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Mohit Sethi (mohitsethi)
[access "refs/for/refs/*"]
	read = group user/Mohit Sethi (mohitsethi)
	push = group user/Mohit Sethi (mohitsethi)
	label-Code-Review = -2..+2 group user/Mohit Sethi (mohitsethi)
