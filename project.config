	rebase = group user/Mohit Sethi (mohitsethi)
	removeReviewer = group user/Mohit Sethi (mohitsethi)
	submit = group user/Mohit Sethi (mohitsethi)
[access "refs/heads/*"]
	read = group user/Mohit Sethi (mohitsethi)
	create = group user/Mohit Sethi (mohitsethi)
	push = +force group user/Mohit Sethi (mohitsethi)
	pushMerge = group user/Mohit Sethi (mohitsethi)
[access "refs/tags/*"]
	push = +force group user/Mohit Sethi (mohitsethi)
	pushTag = group user/Mohit Sethi (mohitsethi)
